{
  "detect": ["aviva"],
  "rules": {
    "plan_number": ["(Plan|Policy) (Number|No\\.)\\s*[:#]?\\s*([A-Z0-9\\-\\/]{6,})", "\\b([A-Z0-9]{8,})\\b"],
    "start_date": ["(Start|Commencement) date\\s*[: ]+([0-3]?\\d[\\/\\-][0-1]?\\d[\\/\\-](?:\\d{2}|\\d{4})|[0-3]?\\d\\s+\\w+\\s+\\d{2,4})"],
    "selected_retirement_age": ["(Selected|Normal) retirement age\\s*[: ]+(\\d{2})"],
    "current_value": ["Current (plan|fund) value\\s*[: ]*(?:GBP|£)?\\s*([0-9,]+(?:\\.[0-9]{2})?)"],
    "current_value_date": ["(As at|Valuation date)\\s*[: ]*([0-3]?\\d[\\/\\-][0-1]?\\d[\\/\\-](?:\\d{2}|\\d{4}))"],
    "transfer_value": ["(Cash equivalent transfer value|Transfer value)\\s*[: ]*(?:GBP|£)?\\s*([0-9,]+(?:\\.[0-9]{2})?)"],
    "transfer_value_date": ["(As at|Valuation date)\\s*[: ]*([0-3]?\\d[\\/\\-][0-1]?\\d[\\/\\-](?:\\d{2}|\\d{4}))"],
    "partial_transfers_allowed": ["Partial transfer(s)? (allowed|available)\\s*[: ]*(Yes|No)"],
    "life_cover": ["Life (cover|assurance)\\s*[: ]*(Yes|No)"],
    "pension_sharing_earmarking": ["(pension sharing|earmarking) (order)?\\s*[: ]*(Yes|No)"],
    "drawdown_options": ["(Drawdown|Retirement) options?\\s*[:\\n]\\s*(.*?)(?:Death|Charges|$)"],
    "death_benefits": ["Death benefits?\\s*[:\\n]\\s*(.*?)(?:Drawdown|Charges|$)"],
    "charges": ["(Charges|Fees)\\s*[:\\n]\\s*(.*?)(?:Death|Drawdown|$)"]
  }
}
